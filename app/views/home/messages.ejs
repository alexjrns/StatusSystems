<div class="row"> 
	<div class="col-12 ">
		<div class="card">
			<div class="card-header">Ãšltimas atividades</div>
			<div class="card-body">
				<ul class="timeline user-timeline-compact">
					<% if(status != undefined){ %>
						<% for(var i=0; i < status.length; i++){ %>
							<% var dat = new Date(status[i].data_cadastro); 
								var day = dat.getDate().toString();
								var month = (dat.getMonth() + 1).toString();
								var year = dat.getFullYear().toString();
								var hours = dat.getHours().toString();
								var minutes = dat.getMinutes().toString();

								if (day.length < 2) day = '0' + day;
								if (month.length < 2) month = '0' + month;

								var datFinal = day + '/' + month + '/' + year + ' - ' + hours + ':' + minutes;
							%>
							<li>
								<div class="user-timeline-date"><%= datFinal %></div>
								<div class="user-timeline-title"><%= status[i].titulo %></div>
								<div class="user-timeline-description"><%= status[i].status %>.</div>
							</li>
						<% } %>
					<% } %>
				</ul>
			</div>
		</div>
	</div>
</div>